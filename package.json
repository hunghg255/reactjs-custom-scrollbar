    "@types/react-dom": "^18.0.4",
    "husky": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "dom-css": "^2.1.0",
    "raf": "^3.4.1"
  },
  "homepage": "https://github.com/hunghg255/reactjs-custom-scrollbar",
  "repository": {
    "type": "git",
    "url": "https://github.com/hunghg255/reactjs-custom-scrollbar"
