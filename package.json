    {
      "path": "dist/reactjs-custom-scrollbar.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/raf": "^3.4.0",
